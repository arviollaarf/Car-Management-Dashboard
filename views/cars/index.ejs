<%- include("../partials/head") %>

<body>
   <h1>Tabel cars</h1>
        <table>
            <th>Id</th>
            <th>Nama</th>
            <th>Harga</th>
            <th>Ukuran</th>
            <th>Image</th>
            <th>Action</th>
                <% data.map((car)=> { %>
                <tr>
                    <td><%= car.id %></td>
                    <td><%= car.nama %></td>
                    <td><%= car.harga %></td>
                    <td><%= car.ukuran %></td>
                    <td><%= car.image %></td>
                     <td>
                        <a href="/admin/cars/edit/<%= car.id %>">
                            <button>Edit</button>
                        </a>
                        <a href="/cars/delete/<%= car.id %>">
                            <button>Delete</button>
                        </a>
                    </td>
                </tr>
                <% }) %>
        </table>

        <a href="/admin/cars/create">
            <button>create cars baru</button>
        </a>
</body>
</html>